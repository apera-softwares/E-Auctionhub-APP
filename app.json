{
  "expo": {
    "name": "E AuctionsHub",
    "slug": "e-auctionshub",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/playstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.diliplovevanshi.exporouterexample",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": ["tez", "phonepe", "paytmmp"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/playstore.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.diliplovevanshi.exporouterexample",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/playstore.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
         "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/playstore.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "build": {
      "preview": {
        "distribution": "internal"
      }
    },
    "extra": {
      "RAZORPAY_KEY": "rzp_live_BpATSjSGBOFGK9",

      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b34173fd-0532-4f5c-ae6d-1d7e14f1660e"
      }
    },
    "owner": "aperrasolutions"
  }
}
