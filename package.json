{
  "name": "e-auctionshub",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "yarn@4.5.0",
  "private": true,
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "check:tamagui": "tamagui check",
    "start": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "preinstall": "npm config set legacy-peer-deps true"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@tamagui/config",
          "@tamagui/lucide-icons",
          "@tamagui/toast",
          "denodeify",
          "dotenv",
          "metro",
          "react-native-element-dropdown",
          "tamagui",
          "react-native-razorpay",
          "react-native-otp-verify"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.0.14",
    "@tamagui/animations-moti": "^1.132.24",
    "@tamagui/checkbox": "^1.132.24",
    "@tamagui/config": "^1.132.24",
    "@tamagui/lucide-icons": "^1.124.8",
    "@tamagui/toast": "^1.124.8",
    "babel-preset-expo": "~54.0.0",
    "burnt": "^0.12.2",
    "denodeify": "^1.2.1",
    "dotenv": "^16.4.7",
    "expo": "^54.0.8",
    "expo-build-properties": "~1.0.8",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.12",
    "expo-device": "~8.0.7",
    "expo-font": "~14.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-notifications": "~0.32.11",
    "expo-router": "~6.0.6",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-task-manager": "~14.0.7",
    "expo-web-browser": "~15.0.7",
    "metro": "^0.83.1",
    "native-notify": "^4.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-confirmation-code-field": "^7.4.0",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-otp-verify": "^1.1.8",
    "react-native-razorpay": "^2.3.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-toast-message": "^2.2.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "tamagui": "^1.132.24"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@expo/metro-runtime": "~6.1.2",
    "@tamagui/babel-plugin": "^1.124.8",
    "@tamagui/cli": "^1.124.8",
    "@tamagui/metro-plugin": "^1.124.8",
    "@types/react": "^19.0.10",
    "@types/react-native-razorpay": "^2.2.6",
    "typescript": "~5.9.2"
  }
}
